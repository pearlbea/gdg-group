{
  "name": "gdg-group",
  "version": "0.1.9",
  "description": "A GDG web component made with Nutmeg Web Component",
  "main": "dist/gdg-group.js",
  "module": "dist/gdg-group.js",
  "unpkg": "dist/gdg-group.min.js",
  "types": "dist/gdg-group.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": ["gdg-group", "nutmeg"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pearlbea/gdg-group"
  },
  "scripts": {
    "build": "npx nutmeg build .",
    "build:production": "npx nutmeg build . --production",
    "prebuild": "npx nutmeg clean .",
    "prepare": "npm run build:production",
    "pretest": "npm run build",
    "serve": "npx nutmeg serve .",
    "test": "npx nutmeg test .",
    "watch": "npx nutmeg watch ."
  },
  "dependencies": {
    "@nutmeg/seed": "0.5.0",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "@nutmeg/cli": "0.11.0"
  }
}
